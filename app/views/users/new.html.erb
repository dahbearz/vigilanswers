<%= form_for @user do |f| %>
  <% @user.errros.full_messages.each do |msg| %>
    <%= msg %>
  <% end %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.test_field :email %>

  <%= f.label :password%>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <% f.submit %>
<% end %>
<%= link_to 'home', root_path %>
