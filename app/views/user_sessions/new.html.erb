<h2>Login</h2>
<%= form_for @user_session do |f| %>
  <% @user_session.errors.full_messages.each do |msg| %>
    <%= msg %>
  <% end %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "login" %>
<% end %>
<%= link_to 'home', root_path %>

